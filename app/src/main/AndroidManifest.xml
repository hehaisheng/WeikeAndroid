        android:supportsRtl="true"
        android:name=".WKBaseApplication"
        tools:replace="android:icon"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">



        </activity>
        
        <activity android:name=".base.BaseActivity">


        </activity>
        <activity android:name=".business.login.LoginActivity">



        </activity>
        <activity android:name=".business.advertisement.AdvActivity">

        </activity>
        <activity android:name=".business.start.StartActivity">


            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>


        </activity>
        <activity android:name=".business.home.HomeActivity">

        </activity>

        <activity android:name=".business.start.GuideActivity"></activity>
        <activity android:name=".business.myself.DataModifyActivity"></activity>
        <activity android:name=".business.log.AddLogActivity"></activity>
        <activity android:name=".business.working.HandleWorkActivity"></activity>
        <activity android:name=".business.client.AddClientActivity"></activity>
        <activity android:name=".business.search.SearchActivity"></activity>

        <receiver android:name=".broadcast.TokenFailedBroadcast">
            <intent-filter>
                <action
                    ></action>
            </intent-filter>
        </receiver>

    </application>

</manifest>